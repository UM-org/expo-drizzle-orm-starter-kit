generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["reactNative"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./app.db"
}

// Your data model

model Player {
  id         Int      @id @default(autoincrement())
  first_name String
  last_name  String
  phone      String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}

model Gift {
  id          Int      @id @default(autoincrement())
  name        String
  initial_qty BigInt
  actual_qty  BigInt
  created_at  DateTime @default(now())
  updated_at  DateTime @default(now())
}
